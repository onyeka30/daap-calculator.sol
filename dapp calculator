pragma solidity >0.8.4 <0.9.0;

contract LearningFunctions {

// uint b = 4;
function multiplycalculator(uint a, uint b) public view returns(uint) {
    uint result = a * b;
    return result;
}
  function subtractcalculatorbyfour(uint a, uint b) public view returns(uint) {
    uint result = a - b;
    return result;
}

 function divitioncalculator(uint a, uint b) public view returns(uint) {
    uint result = a / b;
    return result;
}

 function additioncalculator(uint a, uint b) public view returns(uint) {
    uint result = a + b;
    return result;
}
}
